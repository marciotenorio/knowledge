{
  "Env": "Custom",
  "ConnectionStrings": {
    "db": "Data Source=dotnetlearningdb"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
